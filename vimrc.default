" vim:ft=vim:

let $VIMBASE= $HOME . "/.vim"
set nocompatible

filetype off    

set rtp+=$VIMBASE/bundle/neobundle.vim/

call neobundle#begin(expand($VIMBASE.'/bundle/'))

NeoBundleFetch 'Shougo/neobundle.vim'
NeoBundle 'asek-ll/asek-ll.vim'

call neobundle#end()

filetype plugin on

source $VIMBASE/startup/settings
